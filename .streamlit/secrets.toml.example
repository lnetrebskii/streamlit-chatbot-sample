# Streamlit secrets configuration template for Azure AI Foundry
# Copy this file to secrets.toml and add your actual values

[azure_ai_foundry]
# Azure AI Foundry configuration
proj_endpoint = "your-azure-ai-foundry-endpoint-here"
agent_id = "your-agent-id-here"

# Note: Authentication is handled via MSAL in the app code
# No API key needed - uses OAuth token from Microsoft Entra ID

# You can add other configuration here
[app]
max_tokens = 1000
temperature = 0.7
